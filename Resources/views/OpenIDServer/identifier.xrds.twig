<?xml version="1.0" encoding="UTF-8"?>
<xrds:XRDS
    xmlns:xrds="xri://$xrds"
    xmlns="xri://$xrd*($v*2.0)">
    <XRD>
        <Service priority="0">
            {% for type in types %}
                <Type>{{ type }}</Type>
            {% endfor %}
            <URI>{{ uri }}</URI>
        </Service>
    </XRD>
</xrds:XRDS>
